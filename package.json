{
  "name": "rheroicons",
  "version": "0.1.3",
  "author": "dcruvolo",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf R/*",
    "convert": "Rscript -e \"source('scripts/icons_convert.R')\"",
    "document": "RScript -e 'devtools::check_man()'",
    "uninstall": "RScript -e 'devtools::uninstall()'",
    "install": "RScript -e 'devtools::install()'",
    "build": "Rscript -e 'devtools::build()'",
    "demo_dev" : "parcel inst/rheroicons-demo/src/index.js -d inst/rheroicons-demo/www -o inst/rheroicons-demo/www/rheroicons.min.js --no-source-maps",
    "demo_shiny": "Rscript -e \"shiny::runApp(appDir = './inst/rheroicons-demo', port = 8000, launch.browser = FALSE)\"",
    "demo_build" : "parcel build inst/rheroicons-demo/src/index.js -d inst/rheroicons-demo/www -o inst/rheroicons-demo/www/rheroicons.min.js --no-source-maps",
    "demo_clean": "rm -rf inst/rheroicons-demo/www/rheroicons.min.*",
    "demo_start": "concurrently \"npm run demo_shiny\" \"npm run demo_dev\" ",
    "demo_deploy" : "Rscript -e \"rsconnect::deployApp(appDir = 'inst/rheroicons-demo', account = 'davidruvolo')\"",
    "package": "npm run build && npm run install"
  },
  "devDependencies": {
    "heroicons": "^0.3.6",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "autoprefixer": "^9.7.6",
    "babel-preset-minify": "^0.5.1",
    "concurrently": "^5.2.0",
    "cssnano": "^4.1.10",
    "parcel": "^1.12.4",
    "postcss-modules": "^2.0.0",
    "sass": "^1.26.5"
  },
  "description": "Heroicons for R",
  "keywords" : ["r", "rstats", "shiny", "heroicons"],
  "homepage" : "https://github.com/davidruvolo51/rheroicons",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidruvolo51/rheroicons.git"
  },
  "bugs" : {
    "url": "https://github.com/davidruvolo51/reheroicons/issues"
  }
}
