{
  "name": "rheroicons",
  "version": "0.2.2",
  "author": "dcruvolo",
  "description": "Heroicons for R",
  "license": "MIT",
  "scripts": {
    "shiny": "Rscript -e \"shiny::runApp(appDir = './inst/rheroicons-demo', port = 8000, launch.browser = FALSE)\"",
    "start": "parcel serve ./inst/gallery-assets/src/index.js",
    "build": "parcel build ./inst/gallery-assets/src/index.js",
    "clean": "rm -rf .cache .parcel-cache inst/gallery-assets/public/*",
    "deploy": "Rscript -e \"rsconnect::deployApp(appDir = 'inst/rheroicons-gallery', account = 'davidruvolo')\""
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "heroicons": "^0.4.0",
    "parcel": "^2.0.0-beta.1"
  },
  "assets": "inst/gallery-assets/public/rheroicons.min.js",
  "targets": {
    "assets": {
      "distDir": "inst/gallery-assets/public",
      "sourceMap": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidruvolo51/rheroicons.git"
  },
  "bugs": {
    "url": "https://github.com/davidruvolo51/reheroicons/issues"
  },
  "keywords": [
    "r",
    "rstats",
    "shiny",
    "heroicons"
  ]
}
